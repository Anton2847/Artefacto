<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.artefacto001"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="24"
        android:targetSdkVersion="34" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28" />
    <uses-permission android:name="android.permission.INTERNET" />

    <permission
        android:name="com.example.artefacto001.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.artefacto001.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Artefacto001" >
        <activity
            android:name="com.example.artefacto001.ui.auth.register.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.auth.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.auth.onboarding.OnboardingActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.detail.DetailActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.templearchives.artefacts.ArtefactsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.templearchives.TempleArchiveActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.notifikasi.NotificationsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.artefacto001.ui.ticketcounter.dataticket.payment_method.PaymentMethodActivity"
